.version 5.0
.target sm_60
.entry bench(.param .u64 I){
    .reg .b64   ptr;
    .reg .pred  pi<1>;
    .reg .pred  po<1>;
    .reg .b64   r64i<2>;
    ld.param.u64 ptr, [I];
    cvta.to.global.u64  ptr, ptr;
    setp.ne.u64     pi0, ptr, 0;
    ldu.global.b64 r64i0, [ptr+0];
    ldu.global.b64 r64i1, [ptr+8];
    setp.ne.or.b64 po0, r64i0, r64i1, pi0;
    @po0 st.global.b8 [ptr+0], 0;
}
